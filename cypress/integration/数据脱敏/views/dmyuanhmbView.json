{
    "mainpage" : {
        "new" : "[ng-click:\"source.addFun()\"]",
        "del" : "[ng-click:\"source.deleteFun()\"]",
        "bathauth" : "[ng-click:\"source.openAuthority(\"batch\")]",
        "hadoop" : "[ng-click:\"source.HadoopConfig()\"]"
    },
    "popup" :{
        "confirm":".layui-layer-btn0",
        "cancel":".layui-layer-btn1"
    },
    "search" : {
        "searchtype" : "[ng-model:\"source.listObj.searchPtype\"]",
        "searchsubtype" : "[ng-model:\"source.listObj.searchCType\"]",
        "searchname": ".searchBox > .col-lg-8 > .ng-valid-maxlength",
        "searchusername" : "[ng-model:\"source.listObj.groupId\"]",
        "searchyuan" : "[ng-model:\"source.ioType\"]",
        "searchbtn" : ".col-lg-8 > .btn"
    },
    "newcreate" : {
        "newname" : ":nth-child(1) > .col-lg-6 > .form-control",
        "newtype" : ":nth-child(2) > .col-lg-3 > .form-control",
        "newsubtype" : ":nth-child(3) > .col-lg-3 > .form-control",
        "newredioyuan" : "#a",
        "newrediomubiao" : "#b",
        "newnextbtn" : "[ng-show:\"addModel.step :: \"first\"] > .modal-footer > .btn"
    },
    "relation" : {
        "newtoname" : "#scroll > .form-horizontal > :nth-child(1) > .col-lg-6 > .form-control",
        "newIpadress" : ":nth-child(2) > .col-lg-6 > .form-control",
        "newdatabasename" : ":nth-child(4) > .col-lg-6 > .form-control",
        "newport" : ":nth-child(3) > .col-lg-6 > .form-control",

        "newusername" : ":nth-child(5) > .col-lg-6 > .form-control",
        "newpassword" : ":nth-child(6) > .col-lg-6 > .form-control",
        "newconfirm" : "[ng-if:\"addModel.step :: \"db\"] > .modal-footer > [data-text:\"确定\"]",

        "newcode" : "[ng-show:\"addModel.cType.code ::: \"DBF\" || addModel.cType.code ::: \"XML\" || addModel.cType.code ::: \"JSON\" || addModel.cType.code ::: \"MySQL dump\""] > .col-lg-6 > .form-control",
        "newrediobendi" : "#local",
        "newredioyemiansc" : "#upload",
        "newredioyuanchengwj" : "#remote",
        "newfiledirectory" : ".form-group.ng-scope > .col-lg-6 > .form-control"
        },
    "edit": {
        "yuanedit" : "[ng-click:\"source.editModel(item)\"]",
        "yuaneditname" : ":nth-child(1) > .col-lg-6 > .form-control",
        "yuaneditnextstep" : "[ng-show:\"addModel.step :: \"first\"] > .modal-footer > .btn",
        "yuaneditconfirm" : "[ng-if:\"addModel.step :: \"db\"] > .modal-footer > [data-text:\"确定\"]"
    }
}